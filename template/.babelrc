{
  "presets": [
    [
      "@babel/env", {
        "targets": {
          "browsers": [
            "last 5 versions",
            "Android >= 5", 
            "iOS > 8",
            "safari > 8",
            "ie >= 10"
          ]
        },
        "modules": false,
        "useBuiltIns": "entry",
        "loose": true
      }
    ],
    "@babel/react"
  ]{{#if_eq state 'mobx'}},
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }]
  ]
  {{/if_eq}}
}
